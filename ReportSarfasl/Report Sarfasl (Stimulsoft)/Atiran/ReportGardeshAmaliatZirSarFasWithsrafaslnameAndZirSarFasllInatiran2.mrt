        <Parameters isList="true" count="4">
          <value>fromdate,fromdate,23,0</value>
          <value>todate,todate,23,0</value>
          <value>sarfaslID,sarfaslID,8,0</value>
          <value>zirsarfaslID,zirsarfaslID,8,0</value>
        </Parameters>
        <SqlCommand>select act_zirsarfasls.date,sarfasls.rdf as [rdfsarfasl],zirsarfasls.rdf as [rdfzirsarfasl],act_zirsarfasls.dis,
	act_zirsarfasls.bed,act_zirsarfasls.bes,act_zirsarfasls.bed_bes,abs(act_zirsarfasls.manafter) as [ManAfter],
	cast( "sarfasls"."name" as nvarchar(1000)) as [NameSarfasl], cast("zirsarfasls"."name" as nvarchar(1000)) as [NameZirsarfasl] ,  act_zirsarfasls."user"
,(ISNULL((select sum(act.bed) from act_zirsarfasls as act where act.date &lt; @fromdate and act.rdf_zirsarfasls=zirsarfasls.rdf) ,0)) as bed_gh
,(ISNULL((select sum(act.bes) from act_zirsarfasls as act where act.date &lt; @fromdate and act.rdf_zirsarfasls=zirsarfasls.rdf ),0)) as bes_gh
  from act_zirsarfasls inner join zirsarfasls on act_zirsarfasls.rdf_zirsarfasls=zirsarfasls.rdf
    inner join sarfasls on sarfasls.rdf=zirsarfasls.rdf_sarfasl
	where (date between @fromdate and @todate) and cast( "sarfasls"."name" as nvarchar(1000))=@sarfaslID
and
cast("zirsarfasls"."name" as nvarchar(1000))=@zirsarfaslID</SqlCommand>
